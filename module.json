{
  "name": "Spat Revolution",
  "type": "OSC",
  "path": "Software",
  "version": "0.0.1",
  "description": "Flux Spat Revolution Module",
  "url": "www.spatrevolution.com",
  "downloadURL": "www.spatrevolution.com",
  "hasInput": true,
  "hasOutput": true,
  "hideDefaultCommands": false,
  "hideDefaultParameters": [
  ],
  "defaults": {
    "autoAdd": false,
    "oscInput": {
      "localPort": 8001
    },
    "OSC Outputs": {
      "OSC Output": {
        "local": false,
        "remoteHost": "192.168.1.100",
        "remotePort": 8000
      }
    }
  },
  "parameters": {
  },
  	"scripts": [
		"SpatRevolution.js"
	],

 "values": {
   "Setup":
   {
     "type":"Container",

     "Cartesian":
     {
       "type":"Boolean",
       "description":"Define if cartesian or polar data are displayed\n",
      }

 },
   "Source container":
   {
     "type":"Container",
     "Index":
     {
       "type": "Integer",
       "description":"Specify index of source.\n",
       "min":1,
       "max":128
     },
     "Gain":
     {
       "type":"Float",
       "description":"Source gain.\n",
       "min": -144.5,
       "max": 24.0
     },
     "Mute":
     {
       "type":"Boolean",
       "description":"Mute the source\n"
     },
     "Solo":
     {
       "type":"Boolean",
       "description":"Solo the source\n"
     },
     "LFE":
     {
     "type":"Float",
     "description":"LFE gain.\n",
     "min": -144.5,
     "max": 12.0
      },
     "Position":
     {
       "type":"Point3D",
       "min":[-100,-100,-100],
       "max":[100,100,100]
     },
     "Reverb Enable":
     {
       "type":"Boolean",
       "description":"Enable the all reverberation\n"
     },
     "Early Enable":
     {
       "type":"Boolean",
       "description":"Enable the early reflections\n"
     },
     "Cluster Enable":
     {
       "type":"Boolean",
       "description":"Enable the cluster\n"
     },
     "Tail Enable":
     {
       "type":"Boolean",
       "description":"Enable the tail reverberation\n"
     },
     "Presence":
     {
       "type": "Integer",
       "description":"Set the source presence.\n",
       "min":0,
       "max":120
     },
     "Room presence":
     {
       "type": "Integer",
       "description":"Set the source room presence.\n",
       "min":0,
       "max":120
     },
     "Running Reverberance":
     {
       "type": "Integer",
       "description":"Set the source running reverberance parameter.\n",
       "min":0,
       "max":50
     },
     "Envelopment":
     {
       "type": "Integer",
       "description":"Set the source envelopment.\n",
       "min":0,
       "max":50
     },
     "Warmth":
     {
       "type": "Integer",
       "description":"Set the source warmth.\n",
       "min":0,
       "max":60
     },
     "Brillance":
     {
       "type": "Integer",
       "description":"Set the source brillance.\n",
       "min":0,
       "max":60
     },
     "Yaw":
     {
       "type": "Float",
       "description":"Set the source yaw.\n",
       "min":-180,
       "max":180
     },
     "Pitch":
     {
       "type": "Float",
       "description":"Set the source pitch.\n",
       "min":-90,
       "max":90
     },
     "Aperture":
     {
       "type": "Float",
       "description":"Set the source aperture.\n",
       "min":0,
       "max":180
     },
     "Scale":
     {
       "type": "Float",
       "description":"Set the source scale (for multichannel sources).\n",
       "min":0,
       "max":100
     },
     "Spread":
     {
       "type": "Float",
       "description":"Set the source spread.\n",
       "min":0,
       "max":100
     },
     "Knn":
     {
       "type": "Float",
       "description":"Set the source Knn.\n",
       "min":0,
       "max":100
     }
   }
   },

  "commands": {
  }
}


